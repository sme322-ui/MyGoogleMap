<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MyGoogleMap" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Brown">
        <item name="colorPrimary">#5b362c</item>
        <item name="colorPrimaryDark">#3c1e1a</item>
        <item name="colorAccent">#2c515b</item>
        <item name="colorError">#5b2c3a</item>
    </style>

    <style name="Green">
        <item name="colorPrimary">#689f38</item>
        <item name="colorPrimaryDark">#33691e</item>
        <item name="colorAccent">#6f389f</item>
        <item name="colorError">#9c9f38</item>
    </style>

    <style name="Purple">
        <item name="colorPrimary">#7b1fa2</item>
        <item name="colorPrimaryDark">#4a148c</item>
        <item name="colorAccent">#46a21f</item>
        <item name="colorError">#a21f46</item>
    </style>
</resources>